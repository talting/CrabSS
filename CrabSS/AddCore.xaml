<mah:MetroWindow  xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" x:Class="CrabSS.AddCore"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CrabSS"
        mc:Ignorable="d"
        Title="导入服务器内核" Height="263" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="把内核导进来才能开服（" HorizontalAlignment="Left" Height="66" Margin="10,19,0,0" VerticalAlignment="Top" Width="204" FontSize="16"/>
        <TextBox x:Name="jvav" HorizontalAlignment="Left" Height="34" Margin="19,71,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="561" FontFamily="Segoe UI Semilight" FontSize="16" ToolTip="为空则使用默认Java环境"/>
        <Button x:Name="choosejvav" Content="选择" HorizontalAlignment="Left" Height="48" Margin="598,61,0,0" VerticalAlignment="Top" Width="134" FontSize="16" FontWeight="Normal" Foreground="White" BorderBrush="White" Background="#FF0080FF" Click="choosejvav_Click"/>
        <Button x:Name="verify" Content="验证" HorizontalAlignment="Left" Height="48" Margin="20,159,0,0" VerticalAlignment="Top" Width="134" FontSize="16" FontWeight="Normal" Foreground="White" BorderBrush="White" Background="#FF0080FF" Click="verify_Click"/>
    </Grid>
</mah:MetroWindow>
